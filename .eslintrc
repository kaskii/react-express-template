{
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-extra-parens": [1,"all",{"ignoreJSX":"all"}],
    "valid-jsdoc": 2,
    "accessor-pairs": [1,{"getWithoutSet":false,"setWithoutGet":true}],
    "complexity": [1,20],
    "consistent-return": 2,
    "curly": [2,"all"],
    "default-case": 1,
    "dot-notation": 1,
    "dot-location": [1,"property"],
    "eqeqeq": [2,"allow-null"],
    "guard-for-in": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 1,
    "no-labels": ["error"],
    "no-eq-null": 2,
    "no-eval": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2,{"boolean":true,"number":true,"string":false}],
    "no-implied-eval": 2,
    "no-invalid-this": 1,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-wrappers": 1,
    "no-new": 1,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-return-assign": [2,"except-parens"],
    "no-script-url": 2,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-useless-call": 1,
    "no-void": 2,
    "no-warning-comments": [1,{"terms":["todo"," fixme"],"location":"start"}],
    "no-with": 1,
    "yoda": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 1,
    "no-use-before-define": 2,
    "callback-return": 1,
    "handle-callback-err": [1,"err, error"],
    "no-path-concat": 2,
    "array-bracket-spacing": [2,"never",{}],
    "brace-style": 2,
    "camelcase": 2,
    "comma-spacing": [2,{"after":true}],
    "comma-style": 2,
    "computed-property-spacing": [2,"never"],
    "eol-last": 2,
    "lines-around-comment": [2,{"beforeBlockComment":true}],
    "new-cap": 1,
    "new-parens": 2,
    "no-array-constructor": 1,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2,{"max":2}],
    "no-nested-ternary": 1,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2,"always",{}],
    "operator-linebreak": [2,"after"],
    "padded-blocks": [2,"never"],
    "semi-spacing": [2,{"after":true}],
    "keyword-spacing": [2,{"after": true, "before": true}],
    "space-before-blocks": [2,"always"],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
    }],
    "space-in-parens": [2,"never",{}],
    "space-infix-ops": 2,
    "space-unary-ops": [2,{"words":true}],
    "spaced-comment": [2,"always",{}],
    "arrow-spacing": [2,{"before":true,"after":true}],
    "constructor-super": 2,
    "generator-star-spacing": ["error", {"before": true, "after": false}],
    "no-var": 2,
    "prefer-const": 1,
    "prefer-spread": 1,
    "indent": ["error",4, { "SwitchCase": 1 }],
    "quotes": [2,"single"],
    "semi": ["error","always"]
  }
}